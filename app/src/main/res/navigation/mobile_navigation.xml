<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_maps">

    <fragment
        android:id="@+id/navigation_maps"
        android:name="id.trydev.carimasker.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home">
    </fragment>

    <fragment
        android:id="@+id/navigation_covid"
        android:name="id.trydev.carimasker.ui.dashboard.CovidFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_covid" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="id.trydev.carimasker.ui.akun.AkunFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_akun" />
    <fragment
        android:id="@+id/navigation_chat"
        android:name="id.trydev.carimasker.ChatFragment"
        android:label="fragment_chat"
        tools:layout="@layout/fragment_chat" />
</navigation>